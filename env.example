# Variables de configuracion
#
# Time Zone (e.g "Europe/London") - WWW/CRON 
TIME_ZONE="America/La_Paz"
# MySQL/MariaDB database host - WWW/CRON 
DATABASE_HOST="my.database.server"
# MySQL/MariaDB database user - WWW/CRON 
DATABASE_USER="existing_username"
# MySQL/MariaDB database password - WWW/CRON 
DATABASE_PASS="existing_password"
# MySQL/MariaDB database name - WWW/CRON 
DATABASE_NAME="existing_db_name"
# MySQL/MariaDB database port - WWW/CRON 
DATABASE_PORT=3306  
# MySQL/MariaDB allowed hosts - WWW/CRON
DATABASE_WEBHOST="%"
# For proxy/load-balancers. Path to access phpipam in site URL, http:/url/BASE/ - WWW
# BASE="/"
# Network discovery job interval = 5m,10m,15m,30m,1h,2h,4h,6h,12h - CRON
INTERVAL="1h"
